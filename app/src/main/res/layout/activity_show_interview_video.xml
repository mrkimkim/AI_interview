        android:layout_height="wrap_content"
        android:background="#88000000"
        android:fontFamily="@font/nanumsquareb"
        android:padding="10dp"
        android:text="안녕하세요 제 이름은 김김김이 입니다."
        android:textAlignment="center"
        android:textColor="#ffffffff"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@id/show_interview_result_horizon_guide15"
        app:layout_constraintEnd_toEndOf="@id/show_interview_result_vertical_guide95"
        app:layout_constraintStart_toStartOf="@id/show_interview_result_vertical_guide0" />


    <LinearLayout
