<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="portfolio.projects.mrkimkim.ai_interview.ActivityInterviewVideo">

    <SurfaceView
        android:id="@+id/show_interview_video_surface"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/show_interview_video_hud1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#10000000"
        app:layout_constraintStart_toStartOf="@id/show_interview_result_vertical_guide0"
        app:layout_constraintEnd_toEndOf="@id/show_interview_result_vertical_guide40"
        app:layout_constraintTop_toTopOf="@id/show_interview_result_horizon_guide1">

        <android.support.constraint.Guideline
            android:id="@+id/show_interview_video_emo_guide"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.1"
            />

        <LinearLayout
            android:id="@+id/show_interview_video_emo_gadget"
            android:layout_width="5dp"
            android:layout_height="0dp"
            android:background="#ffa0ff7d"
            android:orientation="horizontal"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent">
        </LinearLayout>

        <TextView
            android:id="@+id/show_interview_video_emo_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:includeFontPadding="false"
            android:text="EMOTION"
            android:textSize="12sp"
            android:textColor="#ffa0ff7d"
            android:fontFamily="@font/nanumsquareeb"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/show_interview_video_emo_gadget"/>

        <TextView
            android:id="@+id/show_interview_video_emo_happy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:includeFontPadding="false"
            android:layout_marginLeft="5dp"
            android:text="HAPPY (98%)"
            android:textColor="#ffffffff"
            android:textSize="12sp"
            android:fontFamily="@font/nanumgothic"
            app:layout_constraintStart_toEndOf="@id/show_interview_video_emo_gadget"
            app:layout_constraintTop_toBottomOf="@id/show_interview_video_emo_title" />

        <TextView
            android:id="@+id/show_interview_video_emo_neutral"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:includeFontPadding="false"
            android:layout_marginLeft="5dp"
            android:text="NEUTRAL (2%)"
            android:textSize="12sp"
            android:textColor="#ffffffff"
            android:fontFamily="@font/nanumgothic"
            app:layout_constraintTop_toBottomOf="@id/show_interview_video_emo_happy"
            app:layout_constraintStart_toEndOf="@id/show_interview_video_emo_gadget"/>

        <TextView
            android:id="@+id/show_interview_video_emo_sad"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:includeFontPadding="false"
            android:layout_marginLeft="5dp"
            android:text="SAD (0%)"
            android:textSize="12sp"
            android:textColor="#ffffffff"
            android:fontFamily="@font/nanumgothic"
            app:layout_constraintTop_toBottomOf="@id/show_interview_video_emo_neutral"
            app:layout_constraintStart_toEndOf="@id/show_interview_video_emo_gadget"/>

        <TextView
            android:id="@+id/show_interview_video_emo_nervous"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginBottom="5dp"
            android:includeFontPadding="false"
            android:text="NERVOUS (0%)"
            android:textSize="12sp"
            android:textColor="#ffffffff"
            android:fontFamily="@font/nanumgothic"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@id/show_interview_video_emo_sad"
            app:layout_constraintStart_toEndOf="@id/show_interview_video_emo_gadget"/>

    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
