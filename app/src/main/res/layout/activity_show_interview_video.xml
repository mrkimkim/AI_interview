        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <SeekBar
        android:id="@+id/show_interview_video_seekbar"
        android:layout_width="0dp"
        android:layout_height="15dp"
        android:padding="0dp"
        android:paddingEnd="0dp"
        android:paddingStart="0dp"
        android:thumb="@drawable/drawable_thumb"
        style="@style/CustomSeekBar"
        app:layout_constraintBottom_toBottomOf="@id/show_interview_result_horizon_guide17"
        app:layout_constraintStart_toStartOf="@id/show_interview_result_vertical_guide0"
        app:layout_constraintEnd_toEndOf="@id/show_interview_result_vertical_guide95"/>

    <TextView
