        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="00:00"
        android:textColor="#ffffffff"
        android:layout_marginTop="5dp"
        app:layout_constraintTop_toBottomOf="@id/show_interview_video_seekbar"
        app:layout_constraintStart_toStartOf="@id/show_interview_result_vertical_guide0"
        />

    <TextView
