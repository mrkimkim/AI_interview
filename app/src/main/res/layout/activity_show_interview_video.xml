        android:layout_height="0dp"
        android:onClick="onToggleEmotion"
        android:alpha="0.8"
        android:src="@drawable/icon_emotion_pressed"
        app:layout_constraintDimensionRatio="W,1:1"
        app:layout_constraintTop_toTopOf="@id/show_interview_result_horizon_guide1"
        app:layout_constraintStart_toStartOf="@id/show_interview_result_vertical_guide80"
        app:layout_constraintEnd_toEndOf="@id/show_interview_result_vertical_guide95"
        />

    <ImageView
        android:id="@+id/show_interview_video_btn_wps"
        android:layout_width="0dp"
