            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:includeFontPadding="false"
            android:layout_marginLeft="5dp"
            android:text="WORD/10s"
            android:fontFamily="@font/nanumsquareeb"
            android:textSize="12sp"
            android:textColor="#ffa0ff7d"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/show_interview_video_wps_gadget"/>

        <TextView
            android:id="@+id/show_interview_video_tv_wps"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginBottom="5dp"
            android:includeFontPadding="false"
            android:text="4.02"
            android:textSize="20sp"
            android:textColor="#ffffffff"
            android:fontFamily="@font/nanumgothicextrabold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@id/show_interview_video_wps_title"
            app:layout_constraintStart_toEndOf="@id/show_interview_video_wps_gadget"/>
    </android.support.constraint.ConstraintLayout>


    <android.support.constraint.ConstraintLayout
        android:id="@+id/show_interview_video_hud3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="#10000000"
        app:layout_constraintStart_toStartOf="@id/show_interview_result_vertical_guide0"
        app:layout_constraintEnd_toStartOf="@id/show_interview_result_vertical_guide40"
        app:layout_constraintTop_toBottomOf="@id/show_interview_video_hud2">

        <android.support.constraint.Guideline
            android:id="@+id/show_interview_video_pitch_guide"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
