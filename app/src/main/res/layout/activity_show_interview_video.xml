        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="#10000000"
        app:layout_constraintStart_toStartOf="@id/show_interview_result_vertical_guide0"
        app:layout_constraintEnd_toEndOf="@id/show_interview_result_vertical_guide40"
        app:layout_constraintTop_toBottomOf="@id/show_interview_video_hud1">

        <android.support.constraint.Guideline
            android:id="@+id/show_interview_video_wps_guide"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
